{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::advterraformbucket67",
        "arn:aws:s3:::advterraformbucket54"
      ],
      "Principal": {
        "AWS": [
          "arn:aws:iam::261951012404:user/Vivek.Ravindranath@unisys.com"
        ]
      }
    },
    {
      "Effect": "Allow",
      "Action": "s3:ReplicateObject",
      "Resource": "arn:aws:s3:::advterraformbucket54/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceArn": "arn:aws:s3:::advterraformbucket67"
        }
      }
    }
  ]
}
